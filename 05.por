programa {
  funcao inicio() {
    
    inteiro num1
    escreva("Informe o primeiro número: ")
    leia(num1)

    inteiro num2
    escreva("Informe o segundo número: ")
    leia(num2)

    se(num1 > num2){
      escreva("O MAIOR número é: ",num1)
    }
    senao{
      escreva("O MAIOR número é: ",num2)
    }
  }
}
